---
import Navbar from "../Components/Navbar.astro";
import Menu from "../Components/Menu.astro";
import PostLayout from "./PostLayout.astro";

interface Props { //TypeScript
    title: string;
}

const { title } = Astro.props;
---

<PostLayout {title}>
    <div>
    <header class="w-full bg-white  ">
        <Navbar Text="Karen"/>
    </header>

    <aside>
        <Menu/>
    </aside>
</div>

    <slot/>
</div>

</PostLayout>