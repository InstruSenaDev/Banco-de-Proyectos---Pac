---
interface Props {
Text: string;
}


const {Text} = Astro.props;
import Buscador from "../Components/Buscador.astro"
---


<nav class="bg-Color_carta flex  p-4 justify-between z-40">
    <div class="flex items-start z-20"><Buscador  type="search"/></div>
   


<div class="flex grid-cols-2 justify-center items-center mr-6">
    <img src="../../public/img/logo.svg" alt="User Avatar" class=" flex rounded-full w-12 h-12 mr-2 justify-end fill-current text-white">
    <span class="text-black">Karen Riascos <br >{Text}</span>
   
</div>

</nav>
